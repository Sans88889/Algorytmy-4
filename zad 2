def find_min_max(array):
    min_value = array[0]
    max_value = array[0]

    for i in range(1, len(array)):
        if array[i] < min_value:
            min_value = array[i]
        if array[i] > max_value:
            max_value = array[i]

    return min_value, max_value

Przykład
my_array = [4, 3, 8, 2, 6, 5]
min_val, max_val = find_min_max(my_array)
print("Najmniejszy element:", min_val)
print("Największy element:", max_val)
